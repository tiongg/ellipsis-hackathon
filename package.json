{
  "name": "template",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "backend:docker-build": "docker build -t ellipsis-hackathon-be -f apps/backend/Dockerfile --network=host .",
    "frontend:docker-build": "docker build -t ellipsis-hackathon-fe -f apps/frontend/Dockerfile --network=host .",
    "backend:docker-run": "docker run -d -p 2000:2000 --env-file ./.env ellipsis-hackathon-be",
    "frontend:docker-run": "docker run -p 3000:3000 --env-file ./.env ellipsis-hackathon-fe",
    "dev": "npx nx run-many --target=dev --parallel 10",
    "typeorm": "ts-node --project ./apps/backend/tsconfig.typeorm-cli.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:run-migrations": "yarn typeorm -d ./apps/backend/src/config/typeorm-cli.config.ts migration:run",
    "db:revert-migration": "yarn typeorm -d ./apps/backend/src/config/typeorm-cli.config.ts migration:revert",
    "db:show-migrations": "yarn typeorm -d ./apps/backend/src/config/typeorm-cli.config.ts migration:show"
  },
  "engineStrict": true,
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
